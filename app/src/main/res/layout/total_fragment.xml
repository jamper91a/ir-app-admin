<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".pages.Inventario.Inventarios.Crear.Step2.tabs.TotalFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical">

        <TextView
            android:id="@+id/txt0"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/tag_leidos"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/txtCant"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="12121"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/txt1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/fecha_del_inventario"
            android:textAlignment="center"
            android:visibility="gone" />

        <TextView
            android:id="@+id/txtLocDes"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textAlignment="center"
            android:visibility="gone" />

        <TextView
            android:id="@+id/txt2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/zona"
            android:textAlignment="center"
            android:visibility="gone" />

        <TextView
            android:id="@+id/txtNum"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=""
            android:textAlignment="center"
            android:visibility="gone" />
    </LinearLayout>

</FrameLayout>